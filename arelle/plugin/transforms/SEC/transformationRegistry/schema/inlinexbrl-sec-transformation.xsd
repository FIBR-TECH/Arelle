<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
  targetNamespace="http://www.sec.gov/inlineXBRL/transformation/2015-08-31" 
  xmlns:ixt-sec="http://www.sec.gov/inlineXBRL/transformation/2015-08-31"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">

<!--
     Duration year.
-->
  
  <xs:simpleType name="duryearType">
    <xs:annotation>
      <xs:documentation>
Duration in decimal years converted to a durationItemType. 
Number of months are computed as 30.4375 days.
Transformed value is in integer years/months/days, days are truncated (no value smaller than a day).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse" />
      <xs:pattern value="[+\-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  
<!--
     Duration month.
-->
  
  <xs:simpleType name="durmonthType">
    <xs:annotation>
      <xs:documentation>
Duration in decimal months.
Number of months are computed as 30.4375 days.
Transformed value is in integer months/days, days are truncated (no value smaller than a day).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse" />
      <xs:pattern value="[+\-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  
<!--
     Duration week.
-->
  
  <xs:simpleType name="durweekType">
    <xs:annotation>
      <xs:documentation>
Duration in decimal weeks.
Transformed value is in integer days, days are truncated (no value smaller than a day).
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse" />
      <xs:pattern value="[+\-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  
<!--
     Duration day.
-->
  
  <xs:simpleType name="durdayType">
    <xs:annotation>
      <xs:documentation>
Duration days.
Transformed value is in integer days/hours, hours are truncated (no value smaller than an hour). 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse" />
      <xs:pattern value="[+\-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  
<!--
     Duration hour.
-->
  
  <xs:simpleType name="durhourType">
    <xs:annotation>
      <xs:documentation>
Duration hours.
Transformed value is in integer hours, hours are truncated (no value smaller than an hour). 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse" />
      <xs:pattern value="[+\-]?([0-9]+(\.[0-9]*)?|\.[0-9]+)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>
  
<!--
     Number INF.
-->
  
  <xs:simpleType name="numinfType">
    <xs:annotation>
      <xs:documentation>
Number infinity (for float or double item types).
Source text for transformation is not validated, resulting lexical value is "inf".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  
<!--
     Number -INF.
-->
  
  <xs:simpleType name="numneginfType">
    <xs:annotation>
      <xs:documentation>
Number negative infinity (for float or double item types).
Source text for transformation is not validated, resulting lexical value is "-inf".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  
<!--
     Number NaN.
-->
  
  <xs:simpleType name="numnanType">
    <xs:annotation>
      <xs:documentation>
Number NaN (not-a-number special value for float or double item types).
Source text for transformation is not validated, resulting lexical value is "NaN".
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  
